{% macro new_row(level, items)%}
<div class="content">
  {% for item in items %}
    <a href="/mds/{{item.mds_number}}">
    <div class="card {{level}}" id="{{item.mds_number}}">
      <div class="number">
        {{item.mds_number}}
      </div>
      <div class="catagory">
        {% if item.assigned_name %}
        {{item.assigned_name}}
        {% else %}
        {{item.mds_name}}
        {% endif %}
      </div>
    </div>
    </a>
  {% endfor %}
</div><br>
{% endmacro %}